#include "seahorn/seahorn.h"
#define N 3

int a[N];

extern int unknown1(void);

int my_abs(int x);

int my_abs(int x) {
  if (x >= 0) {
    return x;
  } else {
    return -x;
  }
}

int main() {
  int i;
  int j = my_abs(unknown1 ());

  for (i = 0; i < N; i++)
    a[i] = j;

  int s = 0;

  for (i = 0; i < N; i++)
    s += a[i];

  sassert(s >= 0);

  return 42;
}